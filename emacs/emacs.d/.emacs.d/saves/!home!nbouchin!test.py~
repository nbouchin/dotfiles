import sys
from termcolor import colored, cprint

with open("commit") as f:
    data = f.readlines()

begin = ["Update:", "Fixe:", "Delete:"]

for line in data:
    words = line.split()
    if not words[0] in begin:
        cprint("Invalid syntax on word \"%s\"\tCommit must begin with on of those prefix: Update, Fixe or Delete" % words[0], 'red')
    else:
        print("Commit \"%s\" is well formated" % line[0:len(line) - 1]
)
f.close()
